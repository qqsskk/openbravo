	<parameter name="ad_client_id" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String("0BB4BA036586421AB4BA6B4272B927A0")]]></defaultValueExpression>
	</parameter>
	<parameter name="startdate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new SimpleDateFormat("dd-mm-yyyy").parse("01-05-2014")]]></defaultValueExpression>
	</parameter>
	<parameter name="enddate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new SimpleDateFormat("dd-mm-yyyy").parse("31-05-2014")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select B.*, B.saldoawal+B.masuk-B.keluar as saldoakhir
from
(select A.product, sum(A.saldoawal) as saldoawal, sum(A.masuk) as masuk, sum(A.keluar) as keluar
from
(select b.name as product, sum(a.movementqty) as saldoawal, 0 masuk, 0 as keluar
from m_transaction a
inner join m_product b on b.m_product_id=a.m_product_id
where a.ad_client_id=$P{ad_client_id}
and a.movementdate<$P{startdate}
group by b.name

union all

select b.name as product, 0 as saldoawal, sum(a.movementqty) as masuk, 0 as keluar
from m_transaction a
inner join m_product b on b.m_product_id=a.m_product_id
where a.ad_client_id=$P{ad_client_id}
and a.movementdate>=$P{startdate} and a.movementdate<=$P{enddate}
and movementqty>0
group by b.name

union all

select b.name as product, 0 as saldoawal, 0 as masuk, sum(a.movementqty*(-1)) as keluar
from m_transaction a
inner join m_product b on b.m_product_id=a.m_product_id
where a.ad_client_id=$P{ad_client_id}
and a.movementdate>=$P{startdate} and a.movementdate<=$P{enddate}
and movementqty<0
group by b.name) A
group by A.product) B
order by B.product]]></queryString>

