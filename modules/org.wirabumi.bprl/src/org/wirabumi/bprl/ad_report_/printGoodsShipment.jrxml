	<queryString><![CDATA[select b.name as pemohon, c.name as kantor,
	(case extract('dow' from a.created)
		when 0 then 'Minggu'
		when 1 then 'Senin'
		when 2 then 'Selasa'
		when 3 then 'Rabu'
		when 4 then 'Kamis'
		when 5 then 'Jumat'
		when 6 then 'Sabtu'
		else 'Invalid'
		end) as hari,
	a.created as tanggalpengajuan,
	e.name as namabarang,
	d.movementqty as jumlah,
	coalesce((select cost from m_costing
	 where m_product_id=e.m_product_id
	 and em_bprl_availableqty>0
	 order by datefrom
	 limit 1),0) as unitcost,
	d.description as keterangan,
	a.movementdate as tanggalserahterima,
	d.em_bprl_requestedqty as jumlahdiminta
from m_inout a
inner join ad_user b on b.ad_user_id = a.createdby
inner join ad_org c on c.ad_org_id=a.ad_org_id
inner join m_inoutline d on d.m_inout_id=a.m_inout_id
inner join m_product e on e.m_product_id=d.m_product_id
where a.m_inout_id=$P{DOCUMENT_ID}
order by d.line]]></queryString>

