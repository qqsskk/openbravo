	<parameter name="enddate" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[select	a.documentno, a.docstatus, coalesce(a.description, b.description) as description, a.startdate,
		k."name" as oldlocation, coalesce(c."name", h."name") as location,
		l."name" as oldcostcenter, coalesce(d."name", i."name") as costcenter,
		coalesce(e."name", j."name") as main_operation, f."name" as workordergroup,
		g."name" as asset,
		b.isassetmovement, b.isassetopname, b.isdisposed, b.estimatedcost, b.description
from cam_workorder a
left join cam_location c on c.cam_location_id=a.cam_location_id
left join c_costcenter d on d.c_costcenter_id=a.c_costcenter_id
left join ma_maint_operation e on e.ma_maint_operation_id=a.ma_maint_operation_id
left join cam_workorder_group f on f.cam_workorder_group_id=a.cam_workorder_group_id
left join cam_workorderasset b on b.cam_workorder_id=a.cam_workorder_id
left join a_asset g on g.a_asset_id=b.a_asset_id
left join cam_location h on h.cam_location_id=b.cam_location_id
left join c_costcenter i on i.c_costcenter_id=b.c_costcenter_id
left join ma_maint_operation j on j.ma_maint_operation_id=b.ma_maint_operation_id
left join cam_location k on k.cam_location_id=b.cam_oldlocation_id
left join c_costcenter l on l.c_costcenter_id=b.c_oldcostcenter_id
where a.startdate>=$P{startdate}
and a.startdate<=$P{enddate}
order by a.startdate,  k."name", l."name", g."name"]]></queryString>

