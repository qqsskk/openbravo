	<queryString><![CDATA[select distinct x."No. Work Order", x."Tanggal Work Order", x."Cost Center Asal", x."Nomor Pembukuan", x."Asset", x."Status" from
(
--asset yang tetap dan pindahan
(select a.documentno as "No. Work Order", a.startdate as "Tanggal Work Order", c.em_bprl_nomorpembukuan as "Nomor Pembukuan", c.value||' - '||c.name as "Asset",
	d.value||' - '||d.name as "Cost Center Asal",
	(case when d.c_costcenter_id=$P{c_costcenter_id} then 'Tetap' else 'Pindahan' end) as "Status"
from cam_workorder a
inner join cam_workorderasset b on b.cam_workorder_id=a.cam_workorder_id
inner join a_asset c on c.a_asset_id=b.a_asset_id
left join c_costcenter d on d.c_costcenter_id=c.em_cam_costcenter_id
where b.c_costcenter_id=$P{c_costcenter_id}
and a.startdate>=$P{startdate}
and a.startdate<=$P{enddate}
order by a.startdate)

--aset yang hilang/belum scan
--cari aset yang kantor 32, tapi not exists di work order pada tanggal segitu

union all

(select null::varchar as "No. Work Order", null::date as "Tanggal Work Order", a.em_bprl_nomorpembukuan as "Nomor Pembukuan", a.value||' - '||a.name as "Asset",
	d.value||' - '||d.name as "Cost Center Asal",
	'Hilang/Belum Scan' as "Status"
from a_asset a
left join c_costcenter d on d.c_costcenter_id=a.em_cam_costcenter_id
where a.em_cam_costcenter_id=$P{c_costcenter_id}
and not exists (select 1 from cam_workorderasset b
			inner join cam_workorder c on c.cam_workorder_id=b.cam_workorder_id
			where b.a_asset_id=a.a_asset_id
			and c.startdate>=$P{startdate}
			and c.startdate<=$P{enddate}
			and b.isassetopname='Y'
			and b.c_costcenter_id=$P{c_costcenter_id}))
) x
where x."Status" like '%'
order by x."No. Work Order", x."Tanggal Work Order", x."Cost Center Asal", x."Nomor Pembukuan", x."Asset", x."Status"]]></queryString>

